[General]
FullMachineName=<%= fqdn %>
SuiteSpotUserID=<%= scope['dirsrv::params::suite_spot_user_id'] %>
SuiteSpotGroup=<%= scope['dirsrv::params::suite_spot_group'] %>
AdminDomain=<%= domain %>
ConfigDirectoryAdminID=<%= config_directory_admin_id %>
ConfigDirectoryAdminPwd=<%= config_directory_admin_pwd %>
ConfigDirectoryLdapURL=<%= config_directory_ldap_url %>

[slapd]
SlapdConfigForMC=Yes
UseExistingMC=0
ServerPort=<%= ldap_port %>
ServerIdentifier=<%= serveridentifier %>
Suffix=<%= suffix %>
RootDN=<%= rootdn %>
RootDNPwd=<%= rootdn_pwd %>
ds_bename=userRoot
AddSampleEntries=No

[admin]
Port=<%= admin_port %>
ServerIpAddress=<%= server_ip_address %>
ServerAdminID=<%= server_admin_id %>
ServerAdminPwd=<%= server_admin_pwd %>
